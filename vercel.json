{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/server",
            "dest": "api/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/api/ws",
            "dest": "/api/server.js"
        }
    ]
}