{
    "version": 2,
    "builds": [
        {
            "src": "api/websocket.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "styles/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/websocket",
            "dest": "/api/websocket.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}